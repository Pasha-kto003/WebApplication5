@model WebApplication5.Models.Phone
@{
    ViewBag.Title = Model.Name;
}

<h2>Телефон @Model.Name</h2>
<div>
    <dl class="dl-horizontal">
        <dt>Идентификатор</dt>
        <dd>@Model.ID</dd>
        <dt>Компания</dt>
        <dd>@Model.Company</dd>
        <dt>Цена</dt>
        <dd>@Model.Price</dd>
        <dt>Операционная система</dt>
        <dd>@Model.ID</dd>
        <dt>Описание</dt>
        <dd>@Model.Description</dd>
        <dt>Страна</dt>
        <dd>@Model.Country</dd>
        <dt>Гарантия</dt>
        <dd>@Model.Garanty</dd>
        <dt>Диагональ экрана</dt>
        <dd>@Model.Diagonal</dd>
        <dt>Процессор</dt>
        <dd>@Model.Processor</dd>
        <dt>Кол-во ядер</dt>
        <dd>@Model.Count_kernels</dd>
        <dt>Версия блютуза</dt>
        <dd>@Model.Bluetooth</dd>
        <dt>NFC</dt>
        <dd>@Model.NFC</dd>
        <dt>Дата выхода</dt>
        <dd>@Model.YearOfRelease</dd>

    </dl>
</div>
