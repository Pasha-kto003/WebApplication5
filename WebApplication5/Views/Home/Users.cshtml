@model IEnumerable<WebApplication5.Models.User>
@{
    ViewBag.Title = "Все пользователи сайта";

}
<table class="table">

    @foreach (var item in Model)
    {
        <thead>
            <tr>
                <th scope="col">Email</th>
                <th scope="col">Пароль</th>
                <th scope="col">Роль</th>
            </tr>
        </thead>
        <tr>
            <td>@item.Email</td>
            <td>@item.Password</td>
            <td>@item.Role</td>
        </tr>
    }
</table>
